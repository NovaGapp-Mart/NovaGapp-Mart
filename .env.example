# Contest payment (required for live Razorpay vote flow)
RAZORPAY_KEY_ID=rzp_live_xxxxxxxxxxxxx
RAZORPAY_KEY_SECRET=xxxxxxxxxxxxxxxxxxxx
# Keep this true for publish. Set false only for local test-mode checkout.
CONTEST_REQUIRE_LIVE_RAZORPAY=true

# Contest sync to Supabase (optional but recommended)
# Use your project URL and SERVICE ROLE key (not anon key)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Optional explicit keys for contest module (fallbacks to SUPABASE_* above)
CONTEST_SUPABASE_URL=
CONTEST_SUPABASE_SERVICE_ROLE_KEY=
